<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <!-- android:drawable="@drawable/extracciones"-->

        <shape>
            <solid
                android:color="@color/coloRose_soft_soft" />
            <stroke
                android:width="1px"
                android:color="@color/coloRose_soft_soft" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item android:state_pressed="true"
        >
        <shape>
            <solid
                android:color="@color/coloRose_soft_soft" />
            <stroke
                android:width="1px"
                android:color="@color/coloRose_soft_soft" />
            <corners android:radius="8dp" />
        </shape>
    </item>
</selector>